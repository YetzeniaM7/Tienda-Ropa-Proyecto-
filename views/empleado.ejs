  <h2 class = "h1" style="text-align: center;">Empleados</h2>



  <table class="table_equipo" id="customers">
  <tr>
    <th>Id_empleado </th>    
    <th>Nombre_empleado</th>
    <th>Apellido_empleado</th>
    <th>Direccion_empleado</th>
    <th>Telefono_empleado</th>
    <th>Correo_empleado</th>
    <th>Fecha_contratacion</th>
  </tr>
  <% resp.forEach(r => { %>
    <tr>
      <td><%= r.Id_empleado  %></td>
      <td><%= r.Nombre_empleado %></td>
      <td><%= r.Apellido_empleado %></td>
      <td><%= r.Direccion_empleado %></td>
      <td><%= r.Telefono_empleado %></td>
      <td><%= r.Correo_empleado %></td>
      <td><%= r.Fecha_contratacion %></td>
    </tr>
  <% }) %>
</table>
