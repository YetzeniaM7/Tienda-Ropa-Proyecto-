    <th>Nombre</th>
    <th>Marca</th>
    <th>Precio</th>
    <th>Cantidad</th>
  </tr>
  <% resp.forEach(r => { %>
    <tr>
      <td><%= r.Id_producto %></td>
      <td><%= r.Nombre_producto  %></td>
      <td><%= r.Marca_producto %></td>
      <td><%= r.Precio_producto %></td>
      <td><%= r.Cantidad_producto %></td>
    </tr>
  <% }) %>
</table>
